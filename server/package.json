{
  "name": "@gambit-chess/server",
  "version": "1.0.0",
  "description": "Server for Gambit Chess game with WebSocket support",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --testPathPattern=integration.test.ts --verbose",
    "test:engine": "jest --testPathPattern=integration.test.ts --verbose --detectOpenHandles",
    "test:all": "jest --verbose",
    "lint": "eslint src/**/*.ts",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "test:integration": "ts-node src/tests/testRunner.ts",
    "test:watch": "jest --watch",
    "docs": "typedoc",
    "docs:clean": "rimraf docs/api && typedoc",
    "clean": "rimraf dist"
  },
  "author": "cango91",
  "license": "MIT",
  "dependencies": {
    "@gambit-chess/shared": "*",
    "@prisma/client": "^6.6.0",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "argon2": "^0.41.1",
    "chess.js": "^1.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.1.0",
    "nodemailer": "^6.10.1",
    "redis": "^4.7.0",
    "sanitize-html": "^2.15.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/redis": "^4.0.11",
    "@types/sanitize-html": "^2.15.0",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "prisma": "^6.6.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "typescript": "^5.8.2"
  }
}
